{
  "files.exclude": {
    ".next": false,
    "node_modules": false,
    ".commitlintrc.json": true,
    ".env.example": true,
    ".eslintrc.json": false,
    ".gitignore": false,
    ".versionrc.json": true,
    "CHANGELOG.md": false,
    "firebase-debug.log": true,
    "firestore-debug.log": true,
    "next-env.d.ts": true,
    "ui-debug.log": true,
    "package-lock.json": false,
    "firebase.json": false,
    "postcss.config.mjs": false,
    "tailwind.config.ts": false,
    "/D:/Personal/yt-catalog": false,
    "tsconfig.json": false,
    "next.config.mjs": false,
    "DEPENDENCIES.md": true,
    ".husky": false
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports.biome": "explicit",
    "source.action.useSortedKeys.biome": "explicit",
    "source.fixAll.biome": "explicit"
  },
  "typescript.preferences.autoImportFileExcludePatterns": ["@radix-ui/*"],
  "deno.enable": false,
  "pinnedProjects.lock": true,
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    "class:list",
    ".*Styles"
  ],
  "todo-tree.filtering.excludeGlobs": [
    "node_modules/**"
  ]
}
